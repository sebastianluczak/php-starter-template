{
    "name": "night/development",
    "type": "project",
    "authors": [
        {
            "name": "Sebastian Łuczak",
            "email": "sebastian.m.luczak@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.3",
        "phpstan/extension-installer": "^1.3"
    },
    "require-dev": {
        "enlightn/security-checker": "1.x-dev",
        "ergebnis/composer-normalize": "dev-main",
        "friendsofphp/php-cs-fixer": "dev-master",
        "kubawerlos/php-cs-fixer-custom-fixers": "^3.11",
        "mockery/mockery": "2.0.x-dev",
        "pestphp/pest": "2.x-dev",
        "pestphp/pest-plugin-type-coverage": "2.x-dev",
        "phparkitect/phparkitect": "dev-main",
        "phpmd/phpmd": "dev-master",
        "phpstan/phpstan": "1.11.x-dev",
        "phpstan/phpstan-mockery": "1.1.x-dev",
        "rector/rector": "dev-main",
        "roave/security-advisories": "dev-latest",
        "slevomat/coding-standard": "^8.0@dev",
        "squizlabs/php_codesniffer": "^3.8"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "ergebnis/composer-normalize": true,
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    }
}
